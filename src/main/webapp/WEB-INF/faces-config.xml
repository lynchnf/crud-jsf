<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">

    <lifecycle>
        <phase-listener>com.psclistens.example.jsf.PhaseLogger</phase-listener>
    </lifecycle>
    <application>
        <message-bundle>com.psclistens.example.jsf.Messages</message-bundle>
        <resource-bundle>
            <base-name>com.psclistens.example.jsf.CrudMessages</base-name>
            <var>msgs</var>
        </resource-bundle>
    </application>
</faces-config>